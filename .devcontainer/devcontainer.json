{
	"name": "Node.js 14",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "14"
		}
	},
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.shell.linux": "/bin/bash"
			},
			"extensions": ["dbaeumer.vscode-eslint", "svelte.svelte-vscode"]
		}
	},
	"postCreateCommand": "npm ci"
}
